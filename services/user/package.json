{
  "name": "user-service",
  "version": "0.0.1",
  "author": "Andrew C. Hopkins",
  "license": "ISC",
  "description": "The media catalog's user service for optional security.",
  "main": "index.ts",
  "keywords": [
    "media-catalog",
    "user-service",
    "authentication"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "ts-node-dev --respawn index.dev.ts",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint-fix": "npm run lint -- --fix",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "db:migrate:undo": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.1",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/lodash.omit": "^4.5.6",
    "@types/node": "^16.4.13",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "module-alias": "^2.2.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash.omit": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "typeorm": "^0.2.36",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "#root": "./src"
  }
}
